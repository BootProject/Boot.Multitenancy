<sessionFactoryConfiguration
          persist="true"
          namespace="Boot">
  <databases>

  <clear/>
  <add name="boot"
        theme = "Boot"
        autoPersist="true"
        dbType="MySql5"
        domains="localhost|192.168.1.80|www.boot.se|boot.se" />

  <add name="devdata"
        theme = "Dev"
        autoPersist="true"
        dbType="MySql5"
        domains="www.boot.com|boot.com" />

    <add name="devcomdata"
         theme = "Net"
         autoPersist="true"
         dbType="MySql5"
         domains="dev.boot.com" />

  <add name="netdata"
        theme = "Net"
        autoPersist="true"
        dbType="MySql5"
        domains="www.boot.net|boot.net" />


  </databases>
</sessionFactoryConfiguration>
